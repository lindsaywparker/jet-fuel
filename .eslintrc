{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-undef": [0],
    "no-unused-vars": [1],
    "arrow-body-style": [0],
    "no-else-return": [1],
    "no-unused-expressions": [0],
    "class-methods-use-this": [0],
    "no-console": [0],
    "func-names": [0],
    "prefer-arrow-callback": [0],
    "no-restricted-syntax": [0],
    "array-callback-return": [0],
    "consistent-return": [0],
    "no-shadow": [0],
  }
}
